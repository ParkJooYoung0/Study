// continue사용한 홀수 구구단
for (let i = 0; i < 10; i++){
    if(i % 2 === 0) continue;
    for (let j = 0; j < 10; j++){
        console.log(i,j,i*j)
    }
}
// continue사용하지 않은 홀수 구구단
for (let i = 1; i < 10; i += 2){
    for (let j = 1; j < 10; j += 2){
        console.log(i,j,i*j)
    }
}